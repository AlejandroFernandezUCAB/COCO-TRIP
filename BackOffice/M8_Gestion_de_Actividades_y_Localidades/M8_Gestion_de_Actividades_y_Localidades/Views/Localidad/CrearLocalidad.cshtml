
@{
    ViewBag.Title = "CrearLocalidad";
}

<h1 align="center">Localidades</h1>
<br>

  <form>
    <div class="form-group">
      <label for="nombreLocalidad">Nombre:</label>
      <input type="text" class="form-control" id="nombreLocalidad">
    </div>
    <div class="form-group">
      <label for="descripcionLocalidad">Descripcion:</label>
      <input type="text" class="form-control" id="descripcionLocalidad">
    </div>

    <button type="submit" class="btn btn-default">Crear</button>
  </form>

